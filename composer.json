{
    "name": "pinoox/pincore",
    "description": "pinoox is a simple environment for web management and development. You can do whatever you want on the web and add your desire features to your website in a simple way",
    "keywords": [
        "PHP",
        "PHP Framework"
    ],
    "type": "project",
    "homepage": "http://www.pinoox.com/",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pinoox/pincore/issues",
        "source": "https://github.com/pinoox/pincore"
    },
    "authors": [
        {
            "name": "Pinoox",
            "email": "dev@pinoox.com"
        }
    ],
    "require": {
		"php": "^8.0.2",
        "illuminate/database": "^8.0",
        "illuminate/events": "^8.0",
        "doctrine/dbal": "^3.5",
        "symfony/finder": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/routing": "6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/error-handler": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "twig/twig": "^3",
        "symfony/serializer": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/templating": "^6.0",
        "nette/php-generator": "^4.0"
	},
    "autoload": {
        "files": [
            "functions/base.php",
            "functions/router.php"
        ],
        "psr-4": {
            "pinoox\\": "/"
        }
    },
    "require-dev": {
        "nunomaduro/collision": "^5"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
